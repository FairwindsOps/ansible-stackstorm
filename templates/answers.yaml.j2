---
  system::hostname: "{{st2_system_hostname}}"
  hubot::env_export:
    ST2_AUTH_USERNAME: "{{st2_chatops_username}}"
    ST2_AUTH_URL: "https://{{st2_web_subdomain}}.{{st2_domain}}:443/auth"
    HUBOT_SLACK_TOKEN: "{{st2_hubot_slack_token}}"
    ST2_WEBUI_URL: "https://{{st2_web_subdomain}}.{{st2_domain}}"
    NODE_TLS_REJECT_UNAUTHORIZED: "{{st2_node_tls_reject_unauthorized}}"
    ST2_AUTH_PASSWORD: "{{st2_chatops_bot_password}}"
    EXPRESS_PORT: "8081"
    HUBOT_LOG_LEVEL: "{{st2_hubot_log_level}}"
    ST2_API: "https://{{st2_web_subdomain}}.{{st2_domain}}/api"
    ST2_HOSTNAME: "{{st2_web_subdomain}}.{{st2_domain}}"
  users:
    admin:
      admin: true
      password: "{{st2_admin_password}}"
    {{st2_chatops_username}}:
      shell: "/bin/false"
      password: "{{st2_chatops_bot_password}}"
  hubot::chat_alias: "!"
  groups:
    robots:
      gid: "6000"
  hubot::dependencies: {}
  st2::stanley::username: "{{st2_stanley_username}}"
  st2::stanley::ssh_private_key:  "{{st2_stanley_ssh_private_key}}"
  st2::cli_api_url:  "https://{{st2_web_subdomain}}.{{st2_domain}}/api"
  st2::stanley::ssh_public_key: "{{st2_stanley_ssh_public_key}}"
  st2::cli_auth_url: "https://{{st2_web_subdomain}}.{{st2_domain}}/auth"
  st2::auth_url: "https://{{st2_web_subdomain}}.{{st2_domain}}/auth"
  st2::kvs:
    st2::server_uuid:
      value: "a1897482-c47d-11e5-b74d-0e64cf723a07"
    st2::collect_anonymous_data:
      value: false
    st2::user_data:
      value: "{}"
  hubot::external_scripts:
    - "hubot-stackstorm"
  st2::api_url: "https://{{st2_web_subdomain}}.{{st2_domain}}/api"
  st2::installer_run: true
  hubot::adapter: "slack"
  hubot::docker: true
